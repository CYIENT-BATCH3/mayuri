#include<stdio.h>
int main()
{
   /* 2D array declaration*/
   int p[4][4];
   /*Counter variables for the loop*/
   int i, j;
   int sum=0;
   for(i=0; i<4; i++)
   {
      for(j=0;j<4;j++)
      {
         printf("Enter value for p[%d][%d]:", i, j);
         scanf("%d", &p[i][j]);
         sum=sum+p[i][j];
      }
   }
   printf("sum of 2D array is: %d",sum);
//   //Displaying array elements
//   printf("Two Dimensional array elements:\n");
//   for(i=0; i<2; i++)
//   {
//       printf("\n");
//       for(j=0;j<2;j++)
//       {
//          printf("%d \t", p[i][j]);
         
//       }
   //}
   return 0;
}
